@model IEnumerable<MVC_VIDEOS.tipo_juego>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in (List<tipo_juego>)ViewBag.tipojuego)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_tipo_juego }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_tipo_juego }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_tipo_juego })
        </td>
    </tr>
}

</table>
